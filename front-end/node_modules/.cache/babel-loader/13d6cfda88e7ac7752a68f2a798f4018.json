AAAA;AAWA,KA1BA;AA2BAE,YA3BA,sBA2BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEAH,uBAFA;;AAAA;AAEAI,wBAFA;AAGA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMAH;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KAnCA;AAoCAI,eApCA,uBAoCAC,KApCA,EAoCA;AACA;AACA,KAtCA;AAuCAC,UAvCA,oBAuCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEAC,wBAFA,GAEA,cAFA;AAGAA;AAHA;AAAA,uBAIAR,mCAJA;;AAAA;AAIAS,kBAJA;AAAA;AAAA,uBAKAT;AACAd,qCADA;AAEAC,qCAFA;AAGAuB;AAHA,kBALA;;AAAA;AAKAC,kBALA;AAUA;AAVA;AAAA;;AAAA;AAAA;AAAA;AAaAV;;AAbA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA;AAtDA;AAtBA","names":["name","data","title","value","file","addItem","items","findTitle","findItem","created","computed","suggestions","methods","selectItem","item","deleteItem","axios","console","editItem","getItems","response","fileChanged","event","upload","formData","r1","path","r2"],"sourceRoot":"src/views","sources":["Admin.vue"],"sourcesContent":["<template>\n<div class=\"admin\">\n      <h1>The Admin Page!</h1>\n    <div class=\"heading\">\n      <div class=\"circle\">1</div>\n      <h2>Add an Item</h2>\n    </div>\n    <div class=\"add\">\n      <div class=\"form\">\n        <input v-model=\"title\" placeholder=\"Title\">\n        <p></p>\n        <input type=\"file\" name=\"photo\" @change=\"fileChanged\">\n        <textarea v-model=\"value\" placeholder=\"Description \"></textarea>\n        <input type=\"file\" name=\"photo\" @change=\"fileChanged\">\n        <button @click=\"upload\">Upload</button>\n      </div>\n      <div class=\"upload\" v-if=\"addItem\">\n        <h2>{{addItem.title}}</h2>\n        <img :src=\"addItem.path\" />\n      </div>\n    </div>\n    <div class=\"heading\">\n      <div class=\"circle\">2</div>\n      <h2>Edit/Delete an Item</h2>\n    </div>\n    <div class=\"edit\">\n      <div class=\"form\">\n        <input v-model=\"findTitle\" placeholder=\"Search\">\n        <div class=\"suggestions\" v-if=\"suggestions.length > 0\">\n          <div class=\"suggestion\" v-for=\"s in suggestions\" :key=\"s.id\" @click=\"selectItem(s)\">{{s.title}}\n          </div>\n        </div>\n      </div>\n      <div class=\"upload\" v-if=\"findItem\">\n        <input v-model=\"findItem.title\">\n        <p></p>\n        <img :src=\"findItem.path\" />\n      </div>\n      <div class=\"actions\" v-if=\"findItem\">\n        <button @click=\"deleteItem(findItem)\">Delete</button>\n        <button @click=\"editItem(findItem)\">Edit</button>\n      </div>\n    </div>\n</div>\n</template>\n<style scoped>\n/* Suggestions */\n.suggestions {\n  width: 200px;\n  border: 1px solid #ccc;\n}\n\n.suggestion {\n  min-height: 20px;\n}\n\n.suggestion:hover {\n  background-color: #5BDEFF;\n  color: #fff;\n}\n.image h2 {\n  font-style: italic;\n  font-size: 1em;\n}\n\n.heading {\n  display: flex;\n  margin-bottom: 20px;\n  margin-top: 20px;\n}\n\n.heading h2 {\n  margin-top: 8px;\n  margin-left: 10px;\n}\n\n.add,\n.edit {\n  display: flex;\n}\n\n.circle {\n  border-radius: 50%;\n  width: 18px;\n  height: 18px;\n  padding: 8px;\n  background: #333;\n  color: #fff;\n  text-align: center\n}\n\n/* Form */\ninput,\ntextarea,\nselect,\nbutton {\n  font-family: 'Montserrat', sans-serif;\n  font-size: 1em;\n}\n\n.form {\n  margin-right: 50px;\n}\n\n/* Uploaded images */\n.upload h2 {\n  margin: 0px;\n}\n\n.upload img {\n  max-width: 300px;\n}\n</style>\n<script>\nimport axios from 'axios';\nexport default {\n  name: 'Admin',\n  data() {\n    return {\n      title: \"\",\n      value: \"\",\n      file: null,\n      addItem: null,\n      items: [],\n      findTitle: \"\",\n      findItem: null,\n    }\n  },\n  created() {\n    this.getItems();\n  },\n  computed: {\n    suggestions() {\n      let items = this.items.filter(item => item.title.toLowerCase().startsWith(this.findTitle.toLowerCase()));\n      return items.sort((a, b) => a.title > b.title);\n    }\n  },\n   methods: {\n    selectItem(item) {\n      this.findTitle = \"\";\n      this.findItem = item;\n    },\n    async deleteItem(item) {\n      try {\n        await axios.delete(\"/api/items/\" + item._id);\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async editItem(item) {\n      try {\n        await axios.put(\"/api/items/\" + item._id, {\n          title: this.findItem.title,\n        });\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async getItems() {\n     try {\n      let response = await axios.get(\"/api/items\");\n      this.items = response.data;\n      return true;\n  } catch (error) {\n    console.log(error);\n  }\n},\n    fileChanged(event) {\n      this.file = event.target.files[0]\n    },\n    async upload() {\n      try {\n        const formData = new FormData();\n        formData.append('photo', this.file, this.file.name, this.file.value)\n        let r1 = await axios.post('/api/photos', formData);\n        let r2 = await axios.post('/api/items', {\n          title: this.title,\n          value: this.value,\n          path: r1.data.path\n        });\n        this.addItem = r2.data;\n       \n      } catch (error) {\n        console.log(error);\n      }\n    },\n  }\n}\n</script>\n"]}]}